{
  "name": "project-template-cli",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "start": "pnpm clean && pnpm build && pnpm execute",
    "clean": "rm -rf build",
    "build": "tsc && tsc-alias",
    "execute": "node build/index.js",
    "dev": "node-dev -r tsconfig-paths/register src/index.ts",
    "lint": "eslint .",
    "prettier": "prettier -c .",
    "prettier-fix": "prettier -w .",
    "mirror-config": "bash scripts/mirror_config.sh",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=12",
    "pnpm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:MohamadKh75/project-template.git"
  },
  "author": "MohamadKh75",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "husky": "^7.0.4",
    "node-dev": "^7.4.2",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.6.5",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  }
}
